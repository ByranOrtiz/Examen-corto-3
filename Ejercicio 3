//Sucesion fibonacci
#include <stdio.h>

int main() {
    int n, a = 0, b = 1, fib;
    printf("Dime cuantos valores queires: ");
    scanf("%d", &n);
    //printf("Serie de Fibonacci:\n");

    for (int i = 1; i <= n; i++) {
        printf("%d ", a);
        fib = a + b;
        a = b;
        b = fib;
    }
    return 0;
}
